Package: CellVoteR
Title: Ensemble-based single-cell typing pipeline
Version: 1.0.0
Authors@R:
    c(
    person("Josh", "Winter-Luke", , "bs23jmw@leeds.ac.uk", 
    role = c("cre"), comment = c(ORCID = "YOUR-ORCID-ID")),
